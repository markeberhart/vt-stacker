{
	"minzoom":0,
	"maxzoom":5,
	"bbox":{
		"north":85,
		"east":180,
		"south":-85,
		"west":-180
    },
    "http-dev-server": {
        "http-dev-port": 8300,
        "run-http-dev": "true",
        "http-dev-dir": "./examples/www"
    },
    "folder-options": {
        "output-temp-files": "./examples/www/temp-files/",
        "output-pbf-files": "./examples/www/tiles/"
    },
    "sources": {
        "countries": {
            "sourceFileType": "shapefile",
            "url": "./examples/shapefiles/ne_10m_admin_0_countries.shp",
            "geom": "polygon",
            "quality": "90%"
        },
        "us_states": {
            "sourceFileType": "shapefile",
            "url": "./examples/shapefiles/gis/cb_2018_us_state_500k/cb_2018_us_state_500k.shp",
            "geom": "polygon",
            "quality": "80%"
        },
        "us_regions": {
            "sourceFileType": "shapefile",
            "url": "./examples/shapefiles/gis/cb_2018_us_region_500k/cb_2018_us_region_500k.shp",
            "geom": "polygon",
            "quality": "80%"
        },
        "us_counties": {
            "sourceFileType": "shapefile",
            "url": "./examples/shapefiles/gis/cb_2018_us_county_500k/cb_2018_us_county_500k.shp",
            "geom": "polygon",
            "quality": "80%"
        },
        "us_districts": {
            "sourceFileType": "shapefile",
            "url": "./examples/shapefiles/gis/cb_2018_us_cd116_20m/cb_2018_us_cd116_20m.shp",
            "geom": "line",
            "quality": "100%"
        },
        "roads": {
            "sourceFileType": "shapefile",
            "url": "./examples/shapefiles/ne_10m_roads.shp",
            "geom": "line",
            "quality": "80%"
        },
        "places": {
            "sourceFileType": "shapefile",
            "url": "./examples/shapefiles/ne_10m_populated_places.shp",
            "geom": "point",
            "quality": "100%"
        }
    }
}